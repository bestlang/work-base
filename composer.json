{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "aliyuncs/oss-sdk-php": "^2.3",
        "baum/baum": "v2.x-dev",
        "bestlang/laracms": "dev-master",
        "doctrine/dbal": "^2.9",
        "endroid/qr-code": "^3.5",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "^1.8",
        "guzzlehttp/guzzle": "~6.0",
        "jacobcyl/ali-oss-storage": "^2.1",
        "jellybool/flysystem-upyun": "^1.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1",
        "sniper/employee": "dev-master",
        "spatie/laravel-permission": "^2.38",
        "tymon/jwt-auth": "1.0.*",
        "upyun/sdk": "^3.3"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5.18"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "secure-http": false
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "bestlang/base": {
            "type": "path",
            "url": "./bestlang/base"
        },
        "bestlang/laracms": {
            "type": "path",
            "url": "./bestlang/laracms"
        },
        "sniper/employee": {
            "type": "path",
            "url": "./sniper/employee"
        },
        "bestlang/wxpay": {
            "type": "path",
            "url": "./bestlang/wxpay"
        },
        "bestlang/laravel-ueditor": {
            "type": "path",
            "url": "./bestlang/laravel-ueditor"
        },
        "bestlang/alipay-easysdk": {
            "type": "path",
            "url": "./bestlang/alipay-easysdk"
        }
    }
}
